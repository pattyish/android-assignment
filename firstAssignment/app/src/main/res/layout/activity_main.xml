        app:layout_constraintStart_toStartOf="@+id/vi2"
        app:layout_constraintTop_toTopOf="@+id/vi2"
        app:layout_constraintVertical_bias="0.471" />

    <Button
        android:id="@+id/cancelJobsNumber"
        android:layout_width="42dp"
        android:layout_height="26dp"
