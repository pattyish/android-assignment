        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vi2"
        app:layout_constraintEnd_toEndOf="@+id/vi2"
        app:layout_constraintHorizontal_bias="0.022"
        app:layout_constraintStart_toStartOf="@+id/vi2"
        app:layout_constraintTop_toTopOf="@+id/vi2"
